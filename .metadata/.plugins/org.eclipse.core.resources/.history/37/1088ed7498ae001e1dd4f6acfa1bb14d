package d_Chapter03;

import java.util.ArrayList;

public class G_ArrayList02 {

	public static void main(String[] args) {
//		ArrayList 생성
		// : 문자열(String) 요소를 담는 배열
		ArrayList<String> items = new ArrayList<>();
		
		items.add("사과" + "책" + "연필" + "시계" + "컵");
		System.out.println(items);
		
		
		
		
		
		
		
		

	}

}
